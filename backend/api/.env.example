# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Supabase Configuration (Required)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_service_role_key

# Frontend URL (for CORS)
# Local development
FRONTEND_URL=http://localhost:3000

# Production (Vercel) - uncomment when deploying
# FRONTEND_URL=https://your-app.vercel.app

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# LavinMQ Configuration (Optional)
RABBITMQ_HOST=leopard.lmq.cloudamqp.com
RABBITMQ_PORT=5672
RABBITMQ_USER=your_username_here
RABBITMQ_PASS=your_password_here
RABBITMQ_VHOST=your_username_here
RABBITMQ_QUEUE=linkedin_profiles
SCORING_QUEUE=scoring_queue

# Default Settings
DEFAULT_REQUIREMENTS_ID=desk_collection
